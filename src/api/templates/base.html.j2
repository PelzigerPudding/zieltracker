<!DOCTYPE html>
<html lang="de">

<head>
    {% block head %}
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">

    <link rel="stylesheet" type="text/css" href="{{ url_for('static', filename='css/base.css') }}" />

    <link rel="shortcut icon" href="/static/images/favicon.png" type="image/png">

    <!-- JQuery -->
    <script type="text/javascript"
        src="{{ url_for('static', filename='libraries/JQuery/jquery-3.5.1.min.js') }}"></script>

    <!-- Popups -->
    <link rel="stylesheet" type="text/css" href="{{ url_for('static', filename='libraries/X-Popup/popup.css') }}" />
    <script type="text/javascript" src="{{ url_for('static', filename='libraries/X-Popup/popup.js') }}"></script>

    <title>{% block title %}{% endblock %}</title>

    {% endblock %}
</head>

<body>
    <div id="content">
        {% block content %} {% endblock %}
    </div>

    <!-- Bootstrap and additional JavaScript -->
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.5.4/dist/umd/popper.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>

    {% block javascript %}
    <script type="text/javascript" src="{{ url_for('static', filename='js/database.js') }}"></script>
    {% endblock %}
</body>

</html>
